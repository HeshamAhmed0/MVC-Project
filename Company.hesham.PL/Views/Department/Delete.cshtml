@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Department</h1>

<form asp-action="delete">
    <div class="mt-3">
        <label asp-for="Code" class="col-form-label"></label>
        <input asp-for="Code" class="form-control" />
    </div>
    <div class="mt-3">
        <label asp-for="Name" class="col-form-label"></label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="mt-3">
        <label asp-for="CreatenIn" class="col-form-label"></label>
        <input asp-for="CreatenIn" class="form-control" />
    </div>
    <div class="mt-3">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="GetAll" asp-controller="Department" class="btn btn-secondary">Back To List</a>
    </div>
</form>


