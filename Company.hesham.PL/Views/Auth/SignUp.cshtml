@model SignUpDto
@{
    ViewData["Title"] = "SignUp";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}

<h1>SignUp</h1>
<div asp-validation-summary="All" class="m-3">
</div>
<div class="main-agileinfo">
	<div class="agileits-top">
		<form action="#" method="post">
			<input class="text" asp-for="UserName" placeholder="Username" required="">
			<input class="text" asp-for="FirstName" placeholder="First Name" required="">
			<input class="text" asp-for="LastName" placeholder="Last Name" required="">
			<input class="text email"asp-for="Email" placeholder="Email" required="">
			<input class="text"asp-for="Password" placeholder="Password" required="">
			<input class="text w3lpass" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
			<div class="wthree-text">
				<label class="anim">
					<input type="checkbox" class="checkbox" asp-for="IsAgree" required="">
					<span>I Agree To The Terms & Conditions</span>
				</label>
				<div class="clear"> </div>
			</div>
			<input type="submit" value="SIGNUP">
		</form>
		<p>Don't have an Account? <a asp-action="SignIn"> Login Now!</a></p>
	</div>
</div>


@section Validation{
	<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

}